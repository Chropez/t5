{{#paper-card as |card|}}
  {{#if imageUrl}}
    {{card.image src=imageUrl alt=name title=name}}
  {{/if}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
      {{#text.headline}}{{nickname}}{{/text.headline}}
      {{#text.subhead}}{{name}}{{/text.subhead}}
    {{/title.text}}
  {{/card.title}}
  {{#card.content}}

  {{/card.content}}
  {{!--{{#card.actions}}
    {{#paper-button}}Edit{{/paper-button}}
    {{#paper-button}}Remove{{/paper-button}}
  {{/card.actions}}--}}
{{/paper-card}}
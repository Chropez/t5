{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{title.media size="md" src=round.winner.imageUrl alt=round.winner.nickname title=round.winner.nickname}}
    {{#title.text as |text|}}
      {{#text.headline}}{{round.date}}{{/text.headline}}
      {{#text.subhead}}
        Winner: {{round.winner.nickname}}
      {{/text.subhead}}
    {{/title.text}}
  {{/card.title}}
{{/paper-card}}
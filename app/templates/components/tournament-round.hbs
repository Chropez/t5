{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{title.media size="md" src=winner.imageUrl alt=winner.nickname title=winner.nickname}}
    {{#title.text as |text|}}
      {{#text.headline}}{{round.date}}{{/text.headline}}
      {{#text.subhead}}
        Winner: {{winner.nickname}}
      {{/text.subhead}}
    {{/title.text}}
  {{/card.title}}
{{/paper-card}}